{
  "name": "prexis",
  "version": "1.0.0",
  "description": "Prexis is a production-ready Node.js API framework built with TypeScript, Express, and Prisma ORM. Features include GraphQL/REST support, JWT authentication, Swagger docs, Winston logging, Docker deployment, and comprehensive security middleware.",
  "keywords": [
    "express",
    "typescript",
    "prisma",
    "graphql",
    "api",
    "rest",
    "nodejs",
    "backend",
    "framework",
    "boilerplate"
  ],
  "author": "fwx5618177",
  "license": "MIT",
  "homepage": "https://github.com/fwx5618177/prexis#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fwx5618177/prexis.git"
  },
  "bugs": {
    "url": "https://github.com/fwx5618177/prexis/issues"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "tsx src/scripts/generate-certs.ts && nodemon",
    "build": "pnpm clean && swc src -d dist --source-maps --copy-files --strip-leading-paths",
    "start": "node dist/server.js",
    "start:cluster": "pm2 start ecosystem.config.js",
    "clean": "rimraf dist",
    "lint": "eslint src --cache",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "certs:generate": "tsx src/scripts/generate-certs.ts",
    "prepare": "husky",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.2",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "envalid": "^8.1.1",
    "express": "^4.22.1",
    "express-rate-limit": "^8.2.1",
    "graphql": "^16.12.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "http2-express-bridge": "^1.0.7",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "opossum": "^9.0.0",
    "redis": "^5.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/hpp": "^0.2.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.19.2",
    "@types/opossum": "^8.1.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitest/coverage-v8": "^4.0.15",
    "autocannon": "^8.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "prisma": "^5.22.0",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vitest": "^4.0.15"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
