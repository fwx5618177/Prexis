{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "target": "es2022",
    "keepClassNames": true,
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"],
      "@config": ["config"],
      "@types": ["types"],
      "@modules/*": ["modules/*"],
      "@shared/*": ["shared/*"],
      "@services/*": ["services/*"],
      "@protocols/*": ["protocols/*"]
    }
  },
  "module": {
    "type": "commonjs"
  }
}
